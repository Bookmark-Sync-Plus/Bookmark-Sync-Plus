{
    "manifest_version": 2,
    "applications": {
      "gecko": {
        "strict_min_version": "57.0"
      }
    },
    "name": "Bookmark Sync Plus",
    "version": "1.0",
    "browser_action": {
      "default_icon": "bookmark.svg",
      "default_title": "Bookmark Sync Plus",
      "size": 64,
      "default_popup": "option.html"
    },
    "background": {
      "scripts": ["main.js"]
    },
    "permissions": [
      "bookmarks"
    ],
    "content_scripts": [
      {
        "matches" : ["<all_urls>"],
        "js": ["netdisks.js"],
        "match_about_blank": true
      }
    ]
  }
  